<h1>Sign in</h1>
<%= form_tag createCart_path do %>
    <table>   
     <tr>  
          <td><%= label_tag :name %></td>
            <td><%= text_field_tag :name, params[:name] %>
        </tr>
        <tr>  
          <td><%= label_tag :password %></td> 
           <td><%= password_field_tag :password %>
        </tr>   
     <tr>  
          <td><%= submit_tag "Sign in" %></td>
        </tr> 
   </table>
   <p> New user? <%= link_to "Sign up now!", new_seller_path %></p>
<% end %>
